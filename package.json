{
  "name": "oclif-keycloak-device-auth-sample",
  "version": "0.0.0",
  "description": "Example of how to implement a device authentication flow in a Command Line Interface (CLI) using oclif and Keycloak!",
  "author": "William Queiroz @wnqueiroz",
  "bin": {
    "cli": "./bin/run"
  },
  "homepage": "https://github.com/wnqueiroz/oclif-keycloak-device-auth-sample",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "wnqueiroz/oclif-keycloak-device-auth-sample",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.22.0",
    "@oclif/plugin-help": "^5",
    "axios": "^1.2.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.16",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.10",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "cli",
    "dirname": "cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/wnqueiroz/oclif-keycloak-device-auth-sample/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
